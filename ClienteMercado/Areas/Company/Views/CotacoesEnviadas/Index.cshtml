                            <table class="table table-bordered table-striped table-condensed flip-content">
                                <thead class="flip-content">
                                    <tr>
                                        <th width="">
                                            Cotação Nº
                                        </th>
                                        <th>
                                            Central Compras
                                        </th>
                                        <th class="numeric">
                                            Respondendo
                                        </th>
                                        <th>
                                            Dt. Envio
                                        </th>
                                        <th class="numeric">
                                            Dt.Encerrou
                                        </th>
                                        <th class="numeric">
                                            Ramo Atividade
                                        </th>
                                        <th class="numeric">
