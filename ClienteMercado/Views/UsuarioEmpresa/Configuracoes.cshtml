@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

    <script>
        $(document).ready(function () {
            //Atribuir valores iniciais
            $('#ID_MEIO_PAGAMENTO').val(3); //Por enquanto será inserido o valor 3 --> 'TERCEIROS' pois estamos implementando a API de uma empresa de cobrança

            //Variáveis globais usadas pelo Jquery
            var listaIdDescricaoAtividades = [];
            var listaIdDescricaoAtividadesOriginal = [];
            var listaIdAtividades = [];

            //Desliga a exibição dos Planos
            $("#descPlano1").hide();
            $("#descPlano2").hide();
            $("#descPlano3").hide();
            $("#descPlano4").hide();
            $("#btn_gravar").hide();

            //Controle de navegação das abas
            $("#btn_teste").click(function(event) {
                if (($("#ID_GRUPO_ATIVIDADES").val() != '') && ($("#RAMOS_ATIVIDADES_SELECIONADOS").val() != '')) {
                    $("#linha_navegacao_ramos_atividade").hide();
                    $("#linha_navegacao_planos_assinatura").show();
                } else {
                    //alert('Atenção!\n\nTodos os campos nesta tela, deverão ser preenchidos.');
                    jAlert('ATENÇÃO!\n\nTodos os campos nesta tela, deverão ser preenchidos.', '  Ok  ');
                }
            });

            $("#btn_teste2").click(function(event) {
                if ($('#ID_CODIGO_TIPO_CONTRATO_COTADA').val() > 0) {
                    $("#linha_navegacao_planos_assinatura").hide();
                    $("#linha_navegacao_meios_pagamento").show();
                } else {
                    //alert('Atenção!\n\nEscolha um Plano de Assinatura.');
                    jAlert('ATENÇÃO!\n\nEscolha um Plano de Assinatura.', '  Ok  ');
                }
            });

            $("#btn_teste3").click(function(event) {
                $("#linha_navegacao_meios_pagamento").hide();
                $("#linha_navegacao_planos_assinatura").show();
            });

            $("#btn_teste2_return").click(function(event) {
                $("#linha_navegacao_planos_assinatura").hide();
                $("#linha_navegacao_ramos_atividade").show();
            });

            $("#btn_voltar_cartao").click(function(event) {
                $("#linha_navegacao_meios_pagamento").hide();
                $("#linha_navegacao_planos_assinatura").show();
            });

            $("#btn_voltar_boleto").click(function(event) {
                $("#linha_navegacao_meios_pagamento").hide();
                $("#linha_navegacao_planos_assinatura").show();
            });

            //Ajax do Autocomplete das Atividades da Empresa
            $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").autocomplete({
                source: function(request, response) {
                    $.ajax({
                        type: "POST",
                        url: "/UsuarioEmpresa/AutoCompleteRamosAtividade",
                        dataType: "json",
                        data: { 'term': $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val(), 'idRamoAtividade': $('#ID_GRUPO_ATIVIDADES').val() },
                        success: function(data) {
                            response($.map(data, function(item) {
                                return { label: item.DESCRICAO_PRODUTO_SERVICO, value: item.ID_CODIGO_PRODUTOS_SERVICOS_EMPRESAS_PROFISSIONAIS }; 
                            }));
                        }
                    });
                },
                select: function(event, ui) {
                    $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val(ui.item.label); 
                    $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA_CONFORME_BANCO").val(ui.item.label);
                    $('#ID_RAMO_ATIVIDADE_EMPRESA').val(ui.item.value);
                    return false;
                },
                minLength: 3,
                messages: {
                    noResults: "",
                    results: ""
                }
            });

            //Alimenta o Grid das Atividades selecionadas
            $('#btn_adicionar').click(function () {
                if ($("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val() != '') {
                    var idLinkExcluir = $("#ID_RAMO_ATIVIDADE_EMPRESA").val();
                    var ramoAtividadeOriginal = $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val();
                    var descricaoBanco = $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA_CONFORME_BANCO").val();

                    listaIdDescricaoAtividades = $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS').val().split(',');
                    listaIdDescricaoAtividadesOriginal = $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS_ORIGINAL').val().split(',');
                    listaIdAtividades = $("#RAMOS_ATIVIDADES_SELECIONADOS").val().split(','); //Transforma string num array

                    if (listaIdAtividades.length <= 5) {
                        if ($.inArray($("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val().toLowerCase(), listaIdDescricaoAtividades) == -1) {
                            //Evita que o id alternativo seja igual a algum id de banco
                            if ((idLinkExcluir == '') || (idLinkExcluir == parseInt(0)) || (ramoAtividadeOriginal != descricaoBanco)) {
                                idLinkExcluir = (listaIdAtividades.length) + "a";
                            }

                            listaIdDescricaoAtividades.push($("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val().toLowerCase());
                            listaIdDescricaoAtividadesOriginal.push(ramoAtividadeOriginal);
                            listaIdAtividades.push(idLinkExcluir);

                            $('<tr><td class="coluna1" width="70%">&nbsp;<img src="../Content/images/play.png">&nbsp;&nbsp;' + $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val() + '</td><td align="center" width="30%"><a href="#" class="remover" id="' + $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val().toLowerCase() + '" border="0" title="Excluir"><img src="../Content/images/mini_x.png"></a></td></tr>').appendTo('#table_atividades');
                            $("#RAMOS_ATIVIDADES_SELECIONADOS").val(listaIdAtividades);
                            $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS').val(listaIdDescricaoAtividades);
                            $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS_ORIGINAL').val(listaIdDescricaoAtividadesOriginal);

                        } else {
                            //alert('Atenção!\n\nVocê está tentando adicionar uma atividade que já existe em sua lista.\nFavor inserir uma atividade diferente.');
                            jAlert('ATENÇÃO!\n\nVocê está tentando adicionar uma atividade que já existe em sua lista.\nFavor inserir uma atividade diferente.', '  Ok  ');
                        }

                    } else {
                        //alert('Atenção!\n\nO limite de 5 Ramos de Atividade foi atingido!\nPara inserir um novo, você terá de excluir alguma das atividades que foram inseridas anteriormente.');
                        jAlert('ATENÇÃO!\n\nO limite de 5 Ramos de Atividade foi atingido!\nPara inserir um novo, você terá de excluir alguma das atividades que foram inseridas anteriormente.', '  Ok  ');
                    }
                }

                //alert('#RAMOS_ATIVIDADES_SELECIONADOS= ' + $('#RAMOS_ATIVIDADES_SELECIONADOS').val());
                //alert('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS= ' + $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS').val());
                //alert('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS_ORIGINAL= '+$('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS_ORIGINAL').val());

                $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").val('');
                $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA_CONFORME_BANCO").val('');
                $("#ID_RAMO_ATIVIDADE_EMPRESA").val(null);
                $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").focus();
            });

            //Remover itens do Grid de Atividades
            $("#table_atividades").on("click", ".remover", function (e) {
                var idExcluir = $(this).attr('id');

                //Transforma string num array
                listaIdAtividades = $("#RAMOS_ATIVIDADES_SELECIONADOS").val().split(',');
                listaIdDescricaoAtividades = $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS').val().split(',');
                listaIdDescricaoAtividadesOriginal = $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS_ORIGINAL').val().split(',');

                var indexDesc = listaIdDescricaoAtividades.indexOf(idExcluir);

                //Removendo atividade do array de ID´s
                listaIdAtividades.splice(indexDesc, 1);
                listaIdDescricaoAtividades.splice(indexDesc, 1);
                listaIdDescricaoAtividadesOriginal.splice(indexDesc, 1);

                //Removendo atividade da Lista exibida na tela
                $(this).closest('tr').remove();

                $("#RAMOS_ATIVIDADES_SELECIONADOS").val(listaIdAtividades);
                $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS').val(listaIdDescricaoAtividades);
                $('#DESCRICAO_RAMOS_ATIVIDADES_SELECIONADOS_ORIGINAL').val(listaIdDescricaoAtividadesOriginal);
                $("#DESCRICAO_RAMO_ATIVIDADE_EMPRESA").focus();
            });

        });

    </script>
