@model ClienteMercado.Models.CadastroEmpresa

@{
    ViewBag.Title = "ClienteMercado";
    Layout = "~/Views/Shared/_Layout_internas.cshtml";
}

<div id="container_configuracoes">

    @using (Html.BeginForm("Configuracoes", "UsuarioEmpresa", new { @id = "formConfiguracoes" }))
